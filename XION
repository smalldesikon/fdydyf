local fpsLastTime = tick()
local fpsText = "ËÆ°ÁÆó‰∏≠..."

spawn(function()
    while wait() do
        fpsCounter += 1
        
        if tick() - fpsLastTime >= 1 then
            fpsText = string.format("%.1f FPS", fpsCounter) -- ÊòæÁ§∫‰∏Ä‰ΩçÂ∞èÊï∞
            fpsCounter = 0
            fpsLastTime = tick()
        end
    end
end)

